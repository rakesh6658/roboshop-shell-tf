[Unit]
Description = User Service
[Service]
User=roboshop
Environment=MONGO=true
Environment=REDIS_URL='redis://redis.joindevops.store:6379'
Environment=MONGO_URL="mongodb://mongodb.joindevops.store:27017/user"
ExecStart=/bin/node /app/server.js
SyslogIdentifier=user

[Install]
WantedBy=multi-user.target
